    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "ts-node": "^10.4.0"
  },
  "devDependencies": {
    "colors": "^1.4.0",
    "concurrently": "^6.5.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/bcrypt": "^5.0.0",
    "@types/colors": "^1.2.1",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.6",
